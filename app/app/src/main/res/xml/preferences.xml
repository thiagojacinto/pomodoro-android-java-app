<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.pavelsikun.seekbarpreference.SeekBarPreferenceCompat
        android:key="com.litepomodoroapp.timer.timer_length"
        android:title="Timer Length"
        android:summary="Select the minute-length of the timer to run: "
        android:defaultValue="25"
        app:msbp_minValue="1"
        app:msbp_maxValue="60"
        app:msbp_interval="1"
        app:msbp_measurementUnit="minutes"
        app:msbp_dialogEnabled="true"
        />

</PreferenceScreen>